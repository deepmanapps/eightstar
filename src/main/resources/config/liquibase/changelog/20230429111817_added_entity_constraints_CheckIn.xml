<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity CheckIn.
    -->
    <changeSet id="20230429111817-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="check_out_id"
                                 baseTableName="check_in"
                                 constraintName="fk_check_in__check_out_id"
                                 referencedColumnNames="id"
                                 referencedTableName="check_out"/>

        <addForeignKeyConstraint baseColumnNames="hotel_id"
                                 baseTableName="check_in"
                                 constraintName="fk_check_in__hotel_id"
                                 referencedColumnNames="id"
                                 referencedTableName="hotel"/>

        <addForeignKeyConstraint baseColumnNames="customer_id"
                                 baseTableName="check_in"
                                 constraintName="fk_check_in__customer_id"
                                 referencedColumnNames="id"
                                 referencedTableName="customer"/>
    </changeSet>
</databaseChangeLog>
